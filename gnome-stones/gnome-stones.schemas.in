<gconfschemafile>

  <schemalist>
      <schema>
      <key>/schemas/apps/gnome-stones/preferences/scroll_method</key>
      <applyto>/apps/gnome-stones/preferences/scroll_method</applyto>
      <owner>gnome-stones</owner>
      <type>string</type>
      <default>atari_scroll</default>
      <locale name="C">
        <short>Scroll method</short>
        <long>The scroll method, valid values are "atari_scroll", "smooth_scroll" and "center_scroll".</long>
      </locale>
      </schema>

      <schema>
      <key>/schemas/apps/gnome-stones/preferences/game_name</key>
      <applyto>/apps/gnome-stones/preferences/game_name</applyto>
      <owner>gnome-stones</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Game name</short>
        <long>The full path and filename of game to load.</long>
      </locale>
      </schema>

      <schema>
      <key>/schemas/apps/gnome-stones/preferences/start_cave</key>
      <applyto>/apps/gnome-stones/preferences/start_cave</applyto>
      <owner>gnome-stones</owner>
      <type>int</type>
      <default>1</default>
      <locale name="C">
        <short>Cave number to start</short>
        <long>Cave number to start.</long>
      </locale>
      </schema>

      <schema>
      <key>/schemas/apps/gnome-stones/preferences/sound_enabled</key>
      <applyto>/apps/gnome-stones/preferences/sound_enabled</applyto>
      <owner>gnome-stones</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
        <short>Enable sound support</short>
        <long>Enable sound support.</long>
      </locale>
      </schema>

      <schema>
      <key>/schemas/apps/gnome-stones/preferences/joystick_device</key>
      <applyto>/apps/gnome-stones/preferences/joystick_device</applyto>
      <owner>gnome-stones</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Joystick device</short>
        <long>Device to use for joystick control.</long>
      </locale>
      </schema>

      <schema>
      <key>/schemas/apps/gnome-stones/preferences/joystick_switch_level</key>
      <applyto>/apps/gnome-stones/preferences/joystick_switch_level</applyto>
      <owner>gnome-stones</owner>
      <type>float</type>
      <default>0.5</default>
      <locale name="C">
        <short>Joystick switch level</short>
        <long>The level an analog joystick must reach to be considered "on" when emulating a digital joystick. This takes a value between 0.0 and 1.0.</long>
      </locale>
      </schema>

  </schemalist>

</gconfschemafile>

